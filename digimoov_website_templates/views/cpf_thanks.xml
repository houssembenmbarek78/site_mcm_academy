    <template id="digimoov_website_templates.cpf_thank_you" name="CPF | DIGIMOOV">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty">
                <div class="container">
                    <div class="row">
                        <div class="row m-0">
                            <div class="col-lg-6">
                                <div class="card-shadow ">
                                    <img src="/digimoov_website_templates/static/img/Financer la formtaion digimoov avec cpf mon compte formation.webp"
                                         class="img-responsive img img-fluid"
                                         data-original-title="" title="" aria-describedby="tooltip508208"/>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <br/>
                                <br/>
                                <br/>
                                <h3 class="text-center"><i class="fa fa-check-circle fa-2x text-success" role="img" aria-label="Success" title="Success"/></h3>
                                <h3 class="text-center">Merci d'avoir ouvert un ticket, notre équipe le prendra en charge dans les plus bref délais.</h3>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>
