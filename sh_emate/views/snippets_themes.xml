<template id="sh_emate_snippet_options" inherit_id="mass_mailing.snippet_options">

    <xpath expr="//div[@data-js='colorpicker']" position="before">
        <div data-js="js_sh_emate_get_product_select_slider" 
        data-selector=".s_cls_sh_emate_product_item_1,.s_cls_sh_emate_product_item_2,.s_cls_sh_emate_product_item_3,
        .s_cls_sh_emate_product_item_4,.s_cls_sh_emate_product_item_5,.s_cls_sh_emate_product_item_6,.s_cls_sh_emate_product_item_7,
        .s_cls_sh_emate_product_item_8,.s_cls_sh_emate_product_item_9,.s_cls_sh_emate_product_item_10,.s_cls_sh_emate_product_item_11,
        .s_cls_sh_emate_product_item_12,.s_cls_sh_emate_product_item_13,.s_cls_sh_emate_product_item_19,.s_cls_sh_emate_product_item_21,
        .s_cls_sh_emate_product_item_22,.s_cls_sh_emate_product_item_23">
            <we-collapse-area>
                <we-toggler><i class="fa fa-folder-open"/> Choose a Config</we-toggler>
                <we-collapse data-no-preview="true">

                		<we-button data-select-class="sh_emate_slider_id_0">No Config</we-button>
                    
            			<t t-set="sh_sliders" t-value="request.env['sh.emate.slider'].search([ ('slider_type','=','product') ])" />
	                    <t t-if="len(sh_sliders)">
       						
       						<t t-foreach="sh_sliders" t-as="sh_slider">	                    	
       						
  								<t t-set="sh_slider_cls" t-value="'sh_emate_slider_id_' + str(sh_slider.id)"/>
                				<we-button t-att-data-select-class="sh_slider_cls">
                					<t t-esc="sh_slider.name"/>
