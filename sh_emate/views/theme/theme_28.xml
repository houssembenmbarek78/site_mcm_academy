                        <tr>
                            <td width="100%" class="o_mail_h_padding o_mail_v_padding o_mail_no_colorpicker p10 menu_link">
                                <center>
                                    <ul>
                                    	<li><a href="#">MY ACCOUNT</a></li>
                                    	<li><a href="#">YOUR SERVICE</a></li>
                                    	<li><a href="#">CAREERS</a></li>
                                    	<li><a href="#">PRIVACY POLICY</a></li>
                                    </ul>
                                </center>
                            </td>
                        </tr>
                        <tr>
                            <td width="100%" class="o_mail_h_padding o_mail_v_padding o_mail_no_colorpicker">
                                <center>
                                    <p class="c_text">Copyright Â© <t t-raw="company_id.name" /> </p>
                                </center>
                            </td>
                        </tr>
                        <tr>
                            <td width="100%" class="o_mail_h_padding o_mail_v_padding o_mail_no_colorpicker p10 social_icon">
                                <center>
                                    <ul>
                                        <li>
                                            <a t-if="company_id.social_facebook" t-att-href="company_id.social_facebook">
                                                <span class="fa fa-facebook"></span>
                                            </a>
                                        </li>

                                        <li>
                                            <a t-if="company_id.social_twitter" t-att-href="company_id.social_twitter">
                                                <span class="fa fa-twitter"></span>
                                            </a>
                                        </li>

                                        <li>
                                            <a t-if="company_id.social_instagram" t-att-href="company_id.social_instagram">
