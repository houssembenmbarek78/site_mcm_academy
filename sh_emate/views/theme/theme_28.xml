                <table align="center" cellspacing="0" cellpadding="0" border="0" class="o_mail_table_styles">
                    <tbody>
                        <tr>
                            <td width="25%" class="o_mail_col_mv o_mail_col_container o_mail_h_padding o_mail_v_padding logo">
                                <a href="#">
                                    <img src="/sh_emate/static/src/img/tmpl_28/logo.png" alt="logo" />
                                </a>
                            </td>
                            <td width="75%" class="text-right o_mail_no_resize o_mail_col_mv o_mail_col_container">
                                <div class="o_mail_header_social menu">
                                    <a href="#" class="ml10">MEN</a>
                                    <a href="#" class="ml10">|</a>
                                    <a href="#" class="ml10">WOMEN</a>
                                    <a href="#" class="ml10">|</a>
