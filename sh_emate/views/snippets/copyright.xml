<?xml version="1.0" encoding="utf-8"?>
<odoo>


<!-- copyright template 1 -->
<template id="sh_emate_copyright_tmpl_1" name="Copyright 1">
	
	<div class="sh_emate_copyright_1">
		<div class="o_mail_block_copyright" data-name="Copyright" id="sh_copyright_1">
	        <div class="o_mail_snippet_general">
	            <table align="center" cellspacing="0" cellpadding="0" border="0" class="o_mail_table_styles">
	                <tbody>
	                    <tr>
	                        <td width="100%" class="o_mail_h_padding o_mail_v_padding o_mail_no_colorpicker">
	                            <center>
			                        <p>Unsbscribe . View in Browser</p>
			                        <p>Copyright - <t t-if="company_id" t-esc="company_id.name"/>
			                        </p>
			                    </center>
	                        </td>
	                    </tr>
	                </tbody>
	            </table>
	        </div>
	    </div>
	</div>
	
</template>
<!-- end copyright template 1  -->

<!-- copyright template 2 -->
<template id="sh_emate_copyright_tmpl_2" name="Copyright 2">
	
	<div class="sh_emate_copyright_2">
		<div class="o_mail_block_footer_social o_mail_footer_social_left" data-name="Footer Left" id="sh_copyright_3">
		    <div class="o_mail_snippet_general">
		        <table align="center" cellspacing="0" cellpadding="0" class="o_mail_table_styles o_mail_full_width_padding p15">
		            <tbody>
		                <tr>
		                    <td class="o_mail_footer_description copyright_content o_mail_col_mv o_mail_col_container">
		                        <div>
		                            <p class="o_mail_footer_copy o_default_snippet_text">Copyright <span class="fa fa-copyright" role="img" aria-label="Copyright" title="Copyright"></span> Softhealer Technologie</p>
		                        </div>
		                    </td>
					        <td class="o_mail_footer_social right-icon pr0 o_mail_col_mv o_mail_col_container"> 
						        <a aria-label="Facebook" title="Facebook" href="#">
						          <span class="fa fa-facebook"></span>
						        </a>
						        <a aria-label="GooglePlus" title="GooglePlus" href="#">
						          <span class="fa fa-google-plus"></span>
						        </a>
						    	<a aria-label="Twitter" title="Twitter" href="#">
						            <span class="fa fa-twitter"></span>
						        </a>
						        <a aria-label="LinkedIn" title="LinkedIn" href="#">
						            <span class="fa fa-linkedin"></span>
						        </a>
					        </td>
		                </tr>
		            </tbody>
		        </table>
		    </div>
		</div>
    </div>       
	
</template>
<!-- end copyright template 2  -->


