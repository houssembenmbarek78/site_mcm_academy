<!-- call_to_action template 1 -->
<template id="sh_emate_calltoaction_tmpl_1" name="Call To Action 1">
	
	<div class="sh_emate_calltoaction_1">
		<div class="o_mail_block_call_to_action" id="sh_call_to_action_2" data-name="Call to Action">
			<div class="o_mail_snippet_general">
	   			<table align="center" cellspacing="0" cellpadding="0" border="0" class="o_mail_table_styles">
	       			<tbody>
	       				<tr>
	           				<td width="70%" class="o_mail_col_mv o_mail_col_container o_mail_h_padding o_mail_v_padding left-content">
	                     		<p>WE ARE PROVIDING</p>
		                    	<h4>BEST FEATURES</h4>
	           				</td>
							<td width="30%" class="text-right o_mail_col_mv o_mail_col_container o_mail_no_resize v_middle">
	   							<div class="btn-right">
		          					<a class="btn_readmore" href="#">READ MORE</a>
	   							</div>
							</td>
	       				</tr>
	   				</tbody>
	   			</table>
			</div>
		</div>	    
	</div>
	
</template>
<!-- end calltoaction template 1  -->


